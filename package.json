{
  "name": "mogbrew",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.9",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler deploy --minify",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint app",
    "fmt": "oxfmt app",
    "fmt:check": "oxfmt --check app",
    "check": "npm run lint && npm run fmt:check && npm run typecheck",
    "test": "vitest run",
    "cf-typegen": "wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply mogbrew --local",
    "db:migrate:remote": "wrangler d1 migrations apply mogbrew",
    "repos:setup": "bash setup-repos.sh",
    "repos:update": "bash setup-repos.sh --update"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-router": "^1.0.0",
    "@tanstack/react-start": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.38.0",
    "lucide-react": "^0.575.0",
    "radix-ui": "^1.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.5.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "latest",
    "@cloudflare/workers-types": "^4.0.0",
    "@tailwindcss/vite": "^4.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "drizzle-kit": "^0.30.0",
    "oxfmt": "latest",
    "oxlint": "latest",
    "postcss": "^8.5.6",
    "shadcn": "^3.8.5",
    "tailwindcss": "^4.2.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0",
    "wrangler": "^4.0.0"
  },
  "optionalDependencies": {
    "@orpc/client": "^1.0.0",
    "@orpc/server": "^1.0.0",
    "@orpc/tanstack-query": "^1.0.0",
    "hono": "^4.0.0"
  }
}
